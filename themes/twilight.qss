/*
Twilight theme for Calculator UI.
*/

QWidget {
    background-color: rgb(100, 80, 220);
}

QLineEdit {
    background-color: rgb(45, 35, 125);
    color: rgb(255, 202, 42);
    selection-background-color: rgb(248, 215, 115);
}

QPushButton {
    background: rgb(60, 50, 145);
    border-color: rgb(248, 215, 115);
    border-style: outset;
    border-radius: 10px;
    border-width: 2px;
    color: white;
    padding: 5px;
}
